{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "sp",
      "kebab-case"
    ],
    "no-trailing-whitespace": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "no-input-rename": false
  }
}
